package leetcode;

public class SellStock {
	public int maxProfit(int[] prices) {
        if (prices.length < 2) 
        	return 0;
        int left = 0;
        int right = prices.length;
        int mid = (left + right)/2;
        int leftMin = arrayMin(prices[left : mid]);
        int rightMax = arrayMax(prices[mid : right]);
    }
}
